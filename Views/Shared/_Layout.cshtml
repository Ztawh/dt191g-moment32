<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>@ViewData["Title"] - CD-samling</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css"/>
    <link rel="stylesheet" href="~/css/main.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/dt191g_moment32.styles.css" asp-append-version="true"/>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css" crossorigin="anonymous" referrerpolicy="no-referrer" />
</head>
<body>
<header>
    <a asp-area="" asp-controller="Artist" asp-action="Index"><h1><i class="fa-solid fa-compact-disc"></i> CD-samling</h1></a>
    <nav>
        <ul class="flex">
            <li><a asp-area="" asp-controller="Artist" asp-action="Index">Hem</a></li>
            <li><a asp-area="" asp-controller="Cd" asp-action="Index">CD-skivor</a></li>
            <li><a asp-area="" asp-controller="Loan" asp-action="Index">Låna</a></li>
        </ul>
    </nav>
</header>
<div class="container">
    <main role="main" class="pb-3">
        @RenderBody()
    </main>
</div>

<footer class="border-top footer text-muted">
    <div class="container">
        &copy; 2022 - dt191g_moment32
    </div>
</footer>
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="~/js/site.js" asp-append-version="true"></script>
@* <script src="https://kit.fontawesome.com/be0f6fe16e.js" crossorigin="anonymous"></script> *@
@await RenderSectionAsync("Scripts", required: false)
</body>
</html>